{
  "npmScope": "chapsana",
  "affected": {
    "defaultBase": "master"
  },
  "implicitDependencies": {
    "workspace.json": "*",
    "package.json": {
      "dependencies": "*",
      "devDependencies": "*"
    },
    "tsconfig.base.json": "*",
    "tslint.json": "*",
    ".eslintrc.json": "*",
    "nx.json": "*"
  },
  "tasksRunnerOptions": {
    "default": {
      "runner": "@nrwl/workspace/tasks-runners/default",
      "options": {
        "cacheableOperations": ["build", "lint", "test", "e2e"]
      }
    }
  },
  "projects": {
    "api": {
      "tags": []
    },
    "api-interfaces": {
      "tags": []
    },
    "myap": {
      "tags": []
    },
    "myap-e2e": {
      "tags": [],
      "implicitDependencies": ["myap"]
    },
    "nextapp": {
      "tags": []
    },
    "nextapp-e2e": {
      "tags": [],
      "implicitDependencies": ["nextapp"]
    },
    "shared-components": {
      "tags": []
    }
  }
}
